{
  "name": "backend",
  "version": "1.0.0",
  "description": "Build backend API using NodeJS TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && tsc -b",
    "clean": "rimraf ./dist",
    "dev": "ts-node-dev --respawn src/index.ts",
    "format": "eslint \"src/**/*.ts\" --fix && prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "start": "node dist/index.js",
    "start:dev": "ts-node src/index.ts",
    "prod": "npm run clean && npm run build && NODE_ENV=production node dist/index.js"
  },
  "keywords": [
    "backend",
    "boilerplate",
    "nodejs",
    "typescript"
  ],
  "author": "Thotsaphon Ben Ruthamnong",
  "license": "MIT",
  "homepage": "https://github.com/blackinno/backend#readme",
  "bugs": {
    "url": "https://github.com/blackinno/backend/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blackinno/backend.git"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.1",
    "@types/node": "^14.11.1",
    "@types/passport": "^1.0.4",
    "@types/passport-facebook": "^2.1.10",
    "@types/passport-google-oauth20": "^2.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/socket.io": "^2.1.11",
    "@types/socket.io-client": "^1.4.33",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-deprecation": "^1.1.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-unused-imports": "^0.1.3",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "prettier-plugin-organize-imports": "^1.1.1",
    "pretty-quick": "^3.0.2",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "argon2": "^0.27.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.6",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rimraf": "^3.0.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
